{
  "rules": {
    "align": false,
    "ban": false,
    "class-name": true,
    "eofline": false,
    "forin": true,
    "indent": [true, 2],
    "label-position": true,
    "label-defined": true,
    "max-line-length": [false, 140],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true,
      "log",
      "debug",
      "dir",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-consecutive-blank-lines": false,
    "no-construct": true,
    "no-debugger": false,
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-string-literal": false,
    "no-trailing-comma": true,
    "no-unreachable": true,
    "no-unused-expression": false,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "no-var-requires": true,
    "quotemark": false,
    "radix": false,
    "typedef": false,
    "typedef-whitespace": false,
    "whitespace": [false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "curly": false,
    "interface-name": false,
    "jsdoc-format": false,
    "no-trailing-whitespace": false,
    "one-line": [false,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "semicolon": false,
    "triple-equals": [false, "allow-null-check"],
    "no-internal-module": true,
    "no-var-requires": true,
    "variable-name": [false,
      "allow-leading-underscore"
    ]
  }
}
